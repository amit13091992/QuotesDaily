<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <color name="colorPrimary">#255d00</color>
    <color name="colorPrimaryDark">#003300</color>
    <color name="colorAccent">#524c00</color>

    <color name="black_overlay">#66000000</color>
    <color name="color_light_green">#e8f5e9</color>
    <color name="colorWhite">#FFFF</color>
    <color name="colorBlack">#000</color>
    <color name="colorGreenDark">#003d00</color>
    <color name="colorGray">#757575</color>
    <color name="gray_light">#bdbdbd</color>

    <!-- Screens background color-->
    <color name="bg_screen1">#ffc107</color>
    <color name="bg_screen2">#64b5f6</color>
    <color name="bg_screen3">#66bb6a</color>
    <color name="bg_screen4">#ff8a80</color>

    <!-- dots inactive colors -->
    <color name="dot_dark_screen1">#ffe54c</color>
